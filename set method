//create an empty set
const set1= new Set([1800,'Pizza']);//constructor.
 console.log(set1);
  const set2= new Set([1,'Pull Neck',1,'Pull Neck'],{count:false});
   console.log(set2.values());//checks if there is an element inside the set.
   console.log(set2.has(1));//has checks whether the element is a set.

   //adding new elements.
   set1.add('Bantu Hotel');
   console.log(set1.values());

   //deleting old elements.
   set1.delete('Bantu Hotel');
   console.log(set1.values());
